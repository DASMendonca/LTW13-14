CREATE TABLE "Address" ("AddressID" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL UNIQUE, "AddressDetail" TEXT NOT NULL UNIQUE, "City" TEXT NOT NULL, "PostalCode1" INTEGER NOT NULL, "PostalCode2" INTEGER NOT NULL, "Country" TEXT NOT NULL);
CREATE TABLE "Customer" ("ConstumerID" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL UNIQUE, "CostumerTaxID" TEXT NOT NULL UNIQUE, "CustomerName" TEXT NOT NULL UNIQUE, "Email" TEXT UNIQUE, "AddressID" INTEGER REFERENCES "sqlitemanager_temp_table_10029608963" ("AddressID") ON UPDATE CASCADE, "Password" TEXT);
CREATE TABLE "Invoice" ("InvoiceNO" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL UNIQUE, "InvoiceDate" DATE NOT NULL DEFAULT CURRENT_DATE, "CustomerID" INTEGER REFERENCES "sqlitemanager_temp_table_5081878531" ("ConstumerID"));
CREATE TABLE "Invoice_Product" ("InvoiceID" INTEGER REFERENCES "sqlitemanager_temp_table_10029608963" ("AddressID"), "ProductID" INTEGER REFERENCES "sqlitemanager_temp_table_10029608963" ("AddressID"), "Quantity" INTEGER);
CREATE TABLE "Product" ("ProductCode" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL UNIQUE, "ProductDescription" TEXT NOT NULL, "UnitPrice" INTEGER NOT NULL, "UnitOfMeasure" TEXT NOT NULL, "ProdutTypeID" INTEGER REFERENCES "ProductType" ("ProductTypeID"));
CREATE TABLE "ProductType" ("ProductTypeID" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL UNIQUE, "ProductTypeDsecription" TEXT NOT NULL, "TaxID" INTEGER REFERENCES "Tax" ("TaxID"));
CREATE TABLE "Tax" ("TaxID" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "TaxValue" INTEGER NOT NULL);
